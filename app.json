{
  "expo": {
    "jsEngine": "hermes",
    "name": "MoneyEasy",
    "slug": "moneyez-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/logo_apk.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/logo_apk.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ezmoney",
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "enableHermes": true
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/logo_app_splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow MoneyEZ to access your photos to save QR codes.",
          "savePhotosPermission": "Allow MoneyEZ to save QR codes to your photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2e67f12f-c626-43a3-b32b-4753239a39dd"
      }
    }
  }
}
